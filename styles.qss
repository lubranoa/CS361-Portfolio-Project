QPushButton {
    background-color: #cce6ff;
    border: solid beige;
    border-style: outset;
    border-width: 2px;
    border-radius: 6px;
    font: bold 12px;
    min-width: 69px;
    padding: 6px;
    margin-left: 4px;
    margin-right: 4px;
}
QPushButton:pressed {
    background-color: #99ceff;
    border-style: inset;
}
